{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "gcc - Build",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"${workspaceRoot}/src/smain.c",
				"-o",
				"${workspaceRoot}/main"
			],
			"options": {
				"cwd": "${workspaceRoot}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			//"detail": "Task generated by Debugger."
		}
	],
	"version": "2.0.0"
}